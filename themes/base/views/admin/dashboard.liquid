
{% layout 'admin/layouts/default' %}

{% block title -%}
	Dashboard
{%- endblock %}

{% block content %}

	<main>
		<h1>Dashboard</h1>
		<p>This is the Home CMS admin dashboard.</p>
		<ul>
			{% for page in pages %}
				<li>
					{% render 'admin/partials/page-summary' with page as page %}
				</li>
			{% endfor %}
		</ul>
	</main>

{% endblock %}
